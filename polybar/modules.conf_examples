hook-1 = sleep 10s && polybar-msg hook aur-ipc 1 >/dev/null 2>&1 &
initial = 2
format-padding = 1
format = <output>
format-foreground = ${colors.yellow}
format-prefix = "/ "
format-prefix-foreground = ${colors.yellow}

