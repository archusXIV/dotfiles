[module/bspwm]
type = internal/bspwm
pin-workspaces = true
inline-mode = false
enable-click = true
enable-scroll = false
reverse-scroll = false
fuzzy-match = false
;label-separator = "/"
;label-separator-foreground = ${colors.red}
; ws-icon-[0-9]+ = label;icon
; Note that the label needs to correspond with the bspwm workspace name
ws-icon-default =
ws-icon-0 = 1;ÔÅÑ
ws-icon-1 = 2;ÔÅª
ws-icon-2 = 3;ÔÜá
ws-icon-3 = 4;ÔÄæ
ws-icon-4 = 5;Ôâ©
ws-icon-5 = 6;ÔÄà
ws-icon-6 = 7;ÔÖÑ
ws-icon-7 = 8;ÔÇÖ
ws-icon-8 = 9;Ôäí
ws-icon-9 = 10;Ôâç
format = <label-state> <label-mode>
label-monitor = %name%
label-mode-padding = 1
label-focused = %name%
label-focused-padding = 1
label-focused-overline = ${colors.ylw2}
label-focused-underline = ${colors.mag2}
label-focused-foreground = ${colors.ylw2}
label-occupied = %icon%
label-occupied-padding = 1
label-occupied-foreground = ${colors.mag2}
label-occupied-underline = ${colors.mag2}
label-urgent = %icon%
label-urgent-padding = 1
label-urgent-foreground = ${colors.mag2}
label-urgent-underline = ${colors.mag2}
label-empty = %icon%
label-empty-padding = 1
label-empty-foreground = ${colors.foreground}
label-empty-underline = ${colors.mag2}
label-dimmed = %icon%
label-dimmed-padding = 1
label-dimmed-focused
label-dimmed-occupied-foreground = ${colors.mag2}
label-dimmed-empty-foreground = ${colors.foreground}
;label-dimmed-urgent
label-dimmed-foreground = ${colors.mag2}
label-dimmed-underline = ${colors.mag2}
; The following labels will be used to indicate the layout/mode
; for the focused workspace. Requires <label-mode>
label-monocle = " ÔÅÆ "
label-monocle-foreground = ${colors.ylw2}
label-tiled = " ÔÇ≤ "
label-tiled-foreground = ${colors.ylw2}
;label-fullscreen = F
;label-fullscreen-foreground = ${colors.red}
label-floating = " Ôãí "
label-floating-foreground = ${colors.cyn}
label-pseudotiled = " Ôãê "
label-pseudotiled-foreground = ${colors.cyn}
label-locked = " ÔÄ£ "
label-locked-foreground = ${colors.cyn}
label-sticky = " Ôâä "
label-sticky-foreground = ${colors.red}
label-private = " ÔÄá "
label-private-foreground = ${colors.red}
label-marked = " ÔÄå "
label-marked-foreground = ${colors.red}

[module/bsp3_layout-ipc]
type = custom/ipc
hook-0 = echo "tiled"
hook-1 = bsp-layout get $(bspc query -D -d .focused.local) | sed 's/-/tiled/'
initial = 1
format = <output>
format-padding = 1
format-foreground = ${xresources.mag}

