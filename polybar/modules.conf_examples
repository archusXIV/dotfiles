[module/xset_repeat-ipc]
type = custom/ipc
hook-0 = echo "%{F#A6E074}ÔÑú%{F-}"
hook-1 = echo "%{F#FF4747}ÔÑú%{F-}"
initial = 1
format-padding = 1
format = <output>
click-left = xset_repeat --disable & disown
click-right = xset_repeat --enable & disown

[module/yt-ipc]
type = custom/ipc
hook-0 = echo ""
hook-1 = cat "$XDG_RUNTIME_DIR"/youtube.txt
initial = 1
format_padding = 1
format = <output>
format-foreground = ${xresources.foreground}
